{
	"name" : "Mapbox MVT",
	"version" : "1.0",
	"description" : "Just-in-time parser for loaded Mapbox Vector Tiles (https://docs.mapbox.com/vector-tiles/specification/)",
	"permissions" : ["declarativeContent", "storage", "activeTab", "downloads"],
	"background" : {
		"scripts" : ["background.js"],
		"persistent" : false
	},
	"page_action" : {
		"default_popup" : "popup.html",
		"default_icon" : {
			"16" : "images/logo16.png",
			"32" : "images/logo32.png",
			"48" : "images/logo48.png",
			"128" : "images/logo128.png"
		}
	},
	"devtools_page" : "devtools.html",
	"icons" : {
		"16" : "images/logo16.png",
		"32" : "images/logo32.png",
		"48" : "images/logo48.png",
		"128" : "images/logo128.png"
	},
	"manifest_version" : 2
}
